# Arch User Repository (AUR)
Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur

# Home Server
Host proxmox
  HostName proxmox.sbalaji.home
  User root
  IdentityFile ~/.ssh/id_ed25519
  ForwardAgent yes
  ForwardX11 no

Host wirehole
  HostName wirehole.sbalaji.home
  User root
  IdentityFile ~/.ssh/id_ed25519
  ForwardAgent yes
  ForwardX11 no
