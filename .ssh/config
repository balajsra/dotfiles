# Arch User Repository (AUR)
Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur

# Home Server
Host casaos
  HostName casaos
  User casaos
  IdentityFile ~/.ssh/id_ed25519
  ForwardAgent yes
  ForwardX11 yes

Host casaos-root
  HostName casaos
  User root
  IdentityFile ~/.ssh/id_ed25519
  ForwardAgent yes
  ForwardX11 yes
